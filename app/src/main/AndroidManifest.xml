<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hotelmanagementsystem_mobile">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.BaseActivity"></activity>
        <activity
            android:name=".activities.facilities_booking.BoardGameCat"
            android:theme="@style/homeTheme" />
        <activity
            android:name=".activities.facilities_booking.GamingRoomCat"
            android:theme="@style/homeTheme" />
        <activity
            android:name=".activities.facilities_booking.SportsCat"
            android:theme="@style/homeTheme" />
        <activity
            android:name=".activities.Account"
            android:theme="@style/homeTheme" />
        <activity android:name=".activities.Signup" />
        <activity android:name=".activities.Login" />
        <activity android:name=".activities.EVouchers" />
        <activity
            android:name=".activities.facilities_booking.Categories"
            android:label="@string/bar_categories"
            android:parentActivityName=".activities.Homepage" />
        <activity android:name=".activities.CheckOut" />
        <activity
            android:name=".activities.CheckInActivity"
            android:theme="@style/Theme.NoActionBar" />
        <activity
            android:name=".activities.Homepage"
            android:theme="@style/homeTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>